#include <iostream>

using namespace std;

void swap(int& x, int& y) // first i have to do a pass by reference example in this function to do a swap

{

int z = x;

x = y;

y = z;

}

void swapbyValue(int x, int y) // here is where i will create a function to propose a pass by value swap

{

int temp;

temp = x;

x = y;   

y = temp;

return;

}

int main()

{

int allen = 15, barry = 25;


#include <iostream>

using namespace std;

void swap(int& x, int& y)

{

int z = x;

x = y;

y = z;

}

void swapbyValue(int x, int y)

{

int temp;

temp = x;

x = y;   

y = temp;

return;

}

int main()

{

int a = 15, b = 25;

cout << "ahhhhhhhhhh";

cout << "a = " << a << " b = " << b << "\n";

//swap(a, b);

swapbyValue(a,b);

cout << "ahhhhhhhhhhhh placeholder";

cout << "a = " << a << " b = " << b << "\n";

  }










